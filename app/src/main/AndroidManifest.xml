<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wfg.presentation">

    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/images"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity" />
        <activity
            android:name=".Second"
            android:label="@string/recvr" />
        <activity
            android:name=".CameraActivity"
            android:label="@string/location1" />
        <activity
            android:name=".navigation"
            android:label="Explore Campus" />
        <activity
            android:name=".HomePage"
            android:label="HCN Home Page" />
        <activity
            android:name=".callSecurity"
            android:label="Call Security" />
        <activity android:name=".showMap"></activity>
    </application>

</manifest>